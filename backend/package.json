{
   "name": "node-express-sqlite-jwt-typescript-typeorm",
   "version": "1.0.0",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^8.0.29",
      "ts-node": "^3.3.0",
      "ts-node-dev": "^1.0.0-pre.57",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.0",
      "@types/cors": "^2.8.5",
      "@types/helmet": "0.0.43",
      "@types/jsonwebtoken": "^8.3.2",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-validator": "^0.9.1",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "helmet": "^3.22.0",
      "jsonwebtoken": "^8.5.1",
      "reflect-metadata": "^0.1.10",
      "sqlite3": "^4.0.3",
      "typeorm": "0.2.17",
      "ts-node": "^3.3.0"
   },
   "scripts": {
      "build": "tsc -p .",
      "postbuild": "cp package.json build/package.json && cd build && npm install --only=production",
      "start": "set debug=* && ts-node-dev --respawn ./src/index.ts --transpileOnly",
      "prod": "tsc && node ./build/index.js",
      "sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --config ormconfig",
      "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
   }
}
